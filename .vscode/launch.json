    "version": "0.2.0",
    "configurations": [
        {
            "name": "run_retail_agent",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.run_agent",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/retail_workflow",
                "./env/.env",
                "./passwords/.env"
            ]
        },
        {
            "name": "run_retail_assistant",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.run",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/retail_workflow",
                "./env/.env",
                "./passwords/.env"
            ]
        },
        {
            "name": "train_retail_workflow",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.train",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/retail_workflow",
                "./env/.env",
                "./passwords/.env"
            ]
        },        
        {
            "name": "run_todo_list",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.run",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/todo_list",
                "./env/.env",
                "./passwords/.env",
                "--startup_action",
                "./examples/todo_list/startup_action.json"
            ]
        },
        {
            "name": "train_todo_list",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.train",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/todo_list",
                "./env/.env",
                "./passwords/.env"
            ]
        },
        {
            "name": "run_hello_world",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.run",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/hello_world",
                "./env/.env",
                "./passwords/.env"
            ]
        },
        {
            "name": "train_hello_world",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.train",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow/examples/hello_world",
                "./env/.env",
                "./passwords/.env"
            ]
        },
        {
            "name": "train_fastworkflow",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.train",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/"
            },
            "args": [
                "./fastworkflow",
                "./env/.env",
                "./passwords/.env"
            ]
        },
        {
            "name": "build_todo_list",
            "type": "debugpy",
            "request": "launch",
            "module": "fastworkflow.build",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/",
                "LOG_LEVEL": "DEBUG"
            },
            "args": [
                "--source-dir", "./fastworkflow/examples/todo_list/application",
                "--output-dir", "./fastworkflow/examples/todo_list/_commands",
                "--context-model-dir", "./fastworkflow/examples/todo_list",
                "--overwrite"
            ]
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
    ]
}